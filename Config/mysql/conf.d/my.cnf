[mysqld]
user=mysql
default-storage-engine = INNODB
character-set-server = utf8
# sercure-file-priv = NULL #mysql8
# default-authentication-plugin = mysql_native_password #mysql8
port = 3306
basedir = /usr
datadir = /var/lib/mysql
tmpdir = /tmp
pid_file = /var/run/mysqld/mysqld.pid
socket = /var/run/mysqld/mysqld.sock
skip-name-resolve

[client]
port = 3306
default-character-set = utf8

[mysql]
no-auto-rehash
default-character-set = utf8
